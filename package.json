{
  "name": "@javalce/eslint-config",
  "version": "0.13.0",
  "description": "Javier's eslint configuration",
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "homepage": "https://github.com/javalce/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/javalce/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/javalce/eslint-config.git"
  },
  "license": "MIT",
  "author": "Javier Valero <javalce29@gmail.com>",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm typegen && tsup",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -t v -p angular",
    "dev": "npx @eslint/config-inspector --config eslint.config.ts",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepack": "pnpm build",
    "prepare": "husky",
    "release": "bumpp && pnpm publish",
    "typecheck": "tsc --noEmit",
    "typegen": "tsx scripts/typegen.ts",
    "version": "pnpm changelog"
  },
  "lint-staged": {
    "*": "prettier -w --ignore-unknown"
  },
  "dependencies": {
    "@babel/core": "^7.28.0",
    "@babel/eslint-parser": "^7.28.0",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@eslint/compat": "^1.3.1",
    "@eslint/js": "^9.30.1",
    "@stylistic/eslint-plugin": "^5.1.0",
    "eslint-flat-config-utils": "^2.1.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-unicorn": "^59.0.1",
    "globals": "^16.3.0",
    "local-pkg": "^1.1.1",
    "typescript-eslint": "^8.36.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/config-inspector": "^1.1.0",
    "@javalce/prettier-config": "^2.2.0",
    "@types/eslint": "^9.6.1",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/node": "^24.0.13",
    "bumpp": "^10.2.0",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.30.1",
    "eslint-typegen": "^2.2.1",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "svelte": "^5.35.6",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "5.8.3"
  },
  "peerDependencies": {
    "@next/eslint-plugin-next": ">=15.0.0",
    "@vitest/eslint-plugin": "^1.3.4",
    "astro-eslint-parser": "^1.2.2",
    "eslint": ">=9.0.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-solid": "^0.14.5",
    "eslint-plugin-svelte": "^3.10.1",
    "eslint-plugin-testing-library": "^7.5.3",
    "eslint-plugin-vue": "<10.3.0",
    "svelte-eslint-parser": "^1.2.0",
    "typescript": ">=5.0.0 <6.0.0",
    "vue-eslint-parser": "^10.2.0"
  },
  "peerDependenciesMeta": {
    "@next/eslint-plugin-next": {
      "optional": true
    },
    "@vitest/eslint-plugin": {
      "optional": true
    },
    "astro-eslint-parser": {
      "optional": true
    },
    "eslint": {
      "optional": true
    },
    "eslint-plugin-astro": {
      "optional": true
    },
    "eslint-plugin-jest": {
      "optional": true
    },
    "eslint-plugin-jsx-a11y": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-react-refresh": {
      "optional": true
    },
    "eslint-plugin-solid": {
      "optional": true
    },
    "eslint-plugin-svelte": {
      "optional": true
    },
    "eslint-plugin-testing-library": {
      "optional": true
    },
    "eslint-plugin-vue": {
      "optional": true
    },
    "svelte-eslint-parser": {
      "optional": true
    },
    "typescript": {
      "optional": true
    },
    "vue-eslint-parser": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
