{
  "name": "@javalce/eslint-config",
  "version": "0.9.0",
  "description": "Javier's eslint configuration",
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "homepage": "https://github.com/javalce/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/javalce/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/javalce/eslint-config.git"
  },
  "license": "MIT",
  "author": "Javier Valero <javalce29@gmail.com>",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": "./bin/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm typegen && tsup --format esm,cjs --clean --dts",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -t v -p angular",
    "cli": "tsx src/cli.ts",
    "dev": "npx @eslint/config-inspector --config eslint.config.ts",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepack": "pnpm build",
    "prepare": "husky",
    "release": "bumpp && pnpm publish",
    "stub": "tsup --format esm",
    "typecheck": "tsc --noEmit",
    "typegen": "esno scripts/typegen.ts",
    "version": "pnpm changelog && git add CHANGELOG.md"
  },
  "lint-staged": {
    "*": "prettier -w --ignore-unknown"
  },
  "dependencies": {
    "@antfu/ni": "^24.3.0",
    "@babel/core": "^7.26.10",
    "@babel/eslint-parser": "^7.27.0",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.1",
    "@eslint/compat": "^1.2.7",
    "@eslint/js": "^9.23.0",
    "@stylistic/eslint-plugin": "^4.2.0",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-flat-config-utils": "^2.0.1",
    "eslint-import-resolver-typescript": "^4.2.3",
    "eslint-plugin-import-x": "^4.9.2",
    "eslint-plugin-unicorn": "^58.0.0",
    "execa": "^9.5.2",
    "globals": "^16.0.0",
    "local-pkg": "^1.1.1",
    "ora": "^8.2.0",
    "prompts": "^2.4.2",
    "typescript-eslint": "^8.28.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/config-inspector": "^1.0.2",
    "@javalce/prettier-config": "^2.1.0",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.13.13",
    "@types/prompts": "^2.4.9",
    "bumpp": "^10.1.0",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.23.0",
    "eslint-typegen": "^2.1.0",
    "esno": "^4.8.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "svelte": "^5.25.3",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "5.8.2"
  },
  "peerDependencies": {
    "@next/eslint-plugin-next": ">=15.0.0",
    "@vitest/eslint-plugin": "^1.1.38",
    "astro-eslint-parser": "^1.2.2",
    "eslint": ">=9.0.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-solid": "^0.14.5",
    "eslint-plugin-svelte": "^3.3.3",
    "eslint-plugin-testing-library": "^7.1.1",
    "eslint-plugin-vue": "^10.0.0",
    "svelte-eslint-parser": "^1.1.0",
    "typescript": ">=5.0.0 <6.0.0",
    "vue-eslint-parser": "^10.1.1"
  },
  "peerDependenciesMeta": {
    "@next/eslint-plugin-next": {
      "optional": true
    },
    "@vitest/eslint-plugin": {
      "optional": true
    },
    "astro-eslint-parser": {
      "optional": true
    },
    "eslint": {
      "optional": true
    },
    "eslint-plugin-astro": {
      "optional": true
    },
    "eslint-plugin-jest": {
      "optional": true
    },
    "eslint-plugin-jsx-a11y": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-solid": {
      "optional": true
    },
    "eslint-plugin-svelte": {
      "optional": true
    },
    "eslint-plugin-testing-library": {
      "optional": true
    },
    "eslint-plugin-vue": {
      "optional": true
    },
    "svelte-eslint-parser": {
      "optional": true
    },
    "typescript": {
      "optional": true
    },
    "vue-eslint-parser": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
