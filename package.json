{
  "name": "@javalce/eslint-config",
  "version": "0.18.1",
  "description": "Javier's eslint configuration",
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "homepage": "https://github.com/javalce/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/javalce/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/javalce/eslint-config.git"
  },
  "license": "MIT",
  "author": "Javier Valero <javalce29@gmail.com>",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./globs": {
      "types": "./dist/globs.d.ts",
      "default": "./dist/globs.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm typegen && tsdown",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -t v -p angular",
    "dev": "npx @eslint/config-inspector --config eslint.config.ts",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepack": "pnpm build",
    "prepare": "husky",
    "release": "bumpp && pnpm publish",
    "typecheck": "tsc --noEmit",
    "typegen": "tsx scripts/typegen.ts",
    "version": "pnpm changelog"
  },
  "lint-staged": {
    "*": "prettier -w --ignore-unknown"
  },
  "dependencies": {
    "@babel/core": "^7.28.4",
    "@babel/eslint-parser": "^7.28.4",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@eslint/compat": "^1.4.0",
    "@eslint/js": "^9.37.0",
    "@stylistic/eslint-plugin": "^5.4.0",
    "eslint-flat-config-utils": "^2.1.4",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-perfectionist": "^4.15.1",
    "eslint-plugin-unicorn": "^61.0.2",
    "globals": "^16.4.0",
    "local-pkg": "^1.1.2",
    "typescript-eslint": "^8.46.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/config-inspector": "^1.3.0",
    "@javalce/prettier-config": "^2.2.0",
    "@types/eslint": "^9.6.1",
    "@types/eslint-plugin-jsx-a11y": "^6.10.1",
    "@types/node": "^24.7.2",
    "bumpp": "^10.3.1",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.37.0",
    "eslint-typegen": "^2.3.0",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "lint-staged": "^16.2.4",
    "prettier": "^3.6.2",
    "svelte": "^5.40.1",
    "tsdown": "^0.15.7",
    "tsx": "^4.20.6",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "@next/eslint-plugin-next": ">=15.0.0",
    "@ngrx/eslint-plugin": "^20.0.1",
    "@vitest/eslint-plugin": "^1.3.20",
    "angular-eslint": "^20.4.0",
    "astro-eslint-parser": "^1.2.2",
    "eslint": ">=9.0.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-solid": "^0.14.5",
    "eslint-plugin-svelte": "^3.12.4",
    "eslint-plugin-testing-library": "^7.13.3",
    "eslint-plugin-vue": "^10.5.1",
    "svelte-eslint-parser": "^1.3.3",
    "typescript": ">=5.0.0 <6.0.0",
    "vue-eslint-parser": "^10.2.0"
  },
  "peerDependenciesMeta": {
    "@next/eslint-plugin-next": {
      "optional": true
    },
    "@ngrx/eslint-plugin": {
      "optional": true
    },
    "@vitest/eslint-plugin": {
      "optional": true
    },
    "angular-eslint": {
      "optional": true
    },
    "astro-eslint-parser": {
      "optional": true
    },
    "eslint": {
      "optional": true
    },
    "eslint-plugin-astro": {
      "optional": true
    },
    "eslint-plugin-jest": {
      "optional": true
    },
    "eslint-plugin-jsx-a11y": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-react-refresh": {
      "optional": true
    },
    "eslint-plugin-solid": {
      "optional": true
    },
    "eslint-plugin-svelte": {
      "optional": true
    },
    "eslint-plugin-testing-library": {
      "optional": true
    },
    "eslint-plugin-vue": {
      "optional": true
    },
    "svelte-eslint-parser": {
      "optional": true
    },
    "typescript": {
      "optional": true
    },
    "vue-eslint-parser": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
