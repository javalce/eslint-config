{
  "name": "javalce-eslint-config",
  "version": "0.1.0",
  "description": "Javier's eslint configuration",
  "keywords": [
    "eslint-config"
  ],
  "homepage": "https://github.com/javalce/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/javalce/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/javalce/eslint-config.git"
  },
  "license": "MIT",
  "author": "Javier Valero <javalce29@gmail.com>",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm typegen && tsup --format esm,cjs --clean --dts",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -t v -p angular",
    "dev": "npx @eslint/config-inspector --config eslint.config.ts",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepack": "pnpm build",
    "prepare": "husky",
    "release": "bumpp && pnpm publish",
    "stub": "tsup --format esm",
    "typecheck": "tsc --noEmit",
    "typegen": "esno scripts/typegen.ts",
    "version": "pnpm changelog && git add CHANGELOG.md"
  },
  "lint-staged": {
    "*": "prettier -w --ignore-unknown"
  },
  "prettier": "@javalce/prettier-config",
  "resolutions": {
    "tsx": "~4.16.2"
  },
  "dependencies": {
    "@babel/core": "^7.24.9",
    "@babel/eslint-parser": "^7.24.8",
    "@eslint/compat": "^1.1.1",
    "@eslint/js": "^9.7.0",
    "eslint-config-flat-gitignore": "^0.1.8",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import-x": "^3.0.1",
    "eslint-plugin-unicorn": "^54.0.0",
    "globals": "^15.8.0",
    "typescript-eslint": "^7.16.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/config-inspector": "^0.5.2",
    "@javalce/prettier-config": "^1.0.1",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.14.11",
    "bumpp": "^9.4.1",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "npm:eslint-ts-patch@9.5.0-0",
    "eslint-flat-config-utils": "^0.2.5",
    "eslint-ts-patch": "9.5.0-0",
    "eslint-typegen": "^0.2.4",
    "esno": "^4.7.0",
    "husky": "^9.1.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.3",
    "tsup": "^8.1.2",
    "tsx": "~4.16.2",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "eslint": ">=8.40.0",
    "typescript": ">=4.8.0 <6"
  },
  "peerDependenciesMeta": {
    "eslint": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  }
}
