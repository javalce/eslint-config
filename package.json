{
  "name": "@javalce/eslint-config",
  "version": "0.8.5",
  "description": "Javier's eslint configuration",
  "keywords": [
    "eslint-config"
  ],
  "homepage": "https://github.com/javalce/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/javalce/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/javalce/eslint-config.git"
  },
  "license": "MIT",
  "author": "Javier Valero <javalce29@gmail.com>",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": "./bin/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm typegen && tsup --format esm,cjs --clean --dts",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -t v -p angular",
    "dev": "npx @eslint/config-inspector --config eslint.config.ts",
    "lint": "eslint --flag unstable_ts_config .",
    "lint:fix": "eslint --flag unstable_ts_config --fix .",
    "prepack": "pnpm build",
    "prepare": "husky",
    "release": "bumpp && pnpm publish",
    "stub": "tsup --format esm",
    "typecheck": "tsc --noEmit",
    "typegen": "esno scripts/typegen.ts",
    "version": "pnpm changelog && git add CHANGELOG.md"
  },
  "lint-staged": {
    "*": "prettier -w --ignore-unknown"
  },
  "prettier": "@javalce/prettier-config",
  "dependencies": {
    "@antfu/ni": "^23.2.0",
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.26.5",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.1",
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@stylistic/eslint-plugin": "^2.13.0",
    "chalk": "^5.4.1",
    "commander": "^13.0.0",
    "eslint-config-flat-gitignore": "^1.0.0",
    "eslint-flat-config-utils": "^1.0.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import-x": "^4.6.1",
    "eslint-plugin-unicorn": "^56.0.1",
    "execa": "^9.5.2",
    "globals": "^15.14.0",
    "local-pkg": "^1.0.0",
    "ora": "^8.1.1",
    "prompts": "^2.4.2",
    "typescript-eslint": "^8.20.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/config-inspector": "^1.0.0",
    "@javalce/prettier-config": "^1.0.3",
    "@types/eslint": "^9.6.1",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.10.7",
    "@types/prompts": "^2.4.9",
    "bumpp": "^9.10.1",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.18.0",
    "eslint-typegen": "^1.0.0",
    "esno": "^4.8.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "lint-staged": "^15.4.1",
    "prettier": "^3.4.2",
    "svelte": "^5.18.0",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "5.7.3"
  },
  "peerDependencies": {
    "@next/eslint-plugin-next": ">=15.0.0",
    "@vitest/eslint-plugin": "^1.1.25",
    "astro-eslint-parser": "^1.1.0",
    "eslint": ">=9.0.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-solid": "^0.14.5",
    "eslint-plugin-svelte": "^2.46.1",
    "eslint-plugin-testing-library": "^7.1.1",
    "eslint-plugin-vue": "^9.32.0",
    "svelte-eslint-parser": "^0.43.0",
    "typescript": ">=5.0.0 <6.0.0",
    "vue-eslint-parser": "^9.4.3"
  },
  "peerDependenciesMeta": {
    "@next/eslint-plugin-next": {
      "optional": true
    },
    "@vitest/eslint-plugin": {
      "optional": true
    },
    "astro-eslint-parser": {
      "optional": true
    },
    "eslint": {
      "optional": true
    },
    "eslint-plugin-astro": {
      "optional": true
    },
    "eslint-plugin-jest": {
      "optional": true
    },
    "eslint-plugin-jsx-a11y": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-solid": {
      "optional": true
    },
    "eslint-plugin-svelte": {
      "optional": true
    },
    "eslint-plugin-testing-library": {
      "optional": true
    },
    "eslint-plugin-vue": {
      "optional": true
    },
    "svelte-eslint-parser": {
      "optional": true
    },
    "typescript": {
      "optional": true
    },
    "vue-eslint-parser": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
