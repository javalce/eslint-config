{
  "name": "@javalce/eslint-config",
  "version": "0.21.1",
  "description": "Javier's eslint configuration",
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "homepage": "https://github.com/javalce/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/javalce/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/javalce/eslint-config.git"
  },
  "license": "MIT",
  "author": "Javier Valero <javalce29@gmail.com>",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./globs": {
      "types": "./dist/globs.d.ts",
      "default": "./dist/globs.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm typegen && tsdown",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -t v -p angular",
    "dev": "npx @eslint/config-inspector --config eslint.config.ts",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepack": "pnpm build",
    "prepare": "husky",
    "release": "bumpp && pnpm publish",
    "typecheck": "tsc --noEmit",
    "typegen": "tsx scripts/typegen.ts",
    "version": "pnpm changelog"
  },
  "lint-staged": {
    "*": "prettier -w --ignore-unknown"
  },
  "dependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@eslint/compat": "^2.0.1",
    "@eslint/js": "^9.39.2",
    "@stylistic/eslint-plugin": "^5.7.0",
    "eslint-flat-config-utils": "^2.1.4",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-perfectionist": "^5.3.1",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^17.0.0",
    "local-pkg": "^1.1.2",
    "typescript-eslint": "^8.52.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@eslint/config-inspector": "^1.4.2",
    "@javalce/prettier-config": "^2.2.0",
    "@types/eslint": "^9.6.1",
    "@types/eslint-plugin-jsx-a11y": "^6.10.1",
    "@types/node": "^24.10.6",
    "bumpp": "^10.3.2",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.39.2",
    "eslint-typegen": "^2.3.0",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "svelte": "^5.46.1",
    "tsdown": "^0.19.0",
    "tsx": "^4.21.0",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "@next/eslint-plugin-next": ">=15.0.0 || >=16.0.0",
    "@ngrx/eslint-plugin": ">=20.0.0",
    "@tanstack/eslint-plugin-query": ">=5.0.0",
    "@tanstack/eslint-plugin-router": ">=1.0.0",
    "@vitest/eslint-plugin": ">=1.0.0",
    "angular-eslint": ">=20.0.0",
    "astro-eslint-parser": ">=1.0.0",
    "eslint": ">=9.0.0",
    "eslint-plugin-astro": ">=1.0.0",
    "eslint-plugin-jest": ">=29.0.0",
    "eslint-plugin-jsx-a11y": ">=6.10.0",
    "eslint-plugin-react": ">=7.0.0",
    "eslint-plugin-react-hooks": ">=7.0.0",
    "eslint-plugin-react-refresh": ">=0.4.24",
    "eslint-plugin-solid": ">=0.14.5",
    "eslint-plugin-svelte": ">=3.0.0",
    "eslint-plugin-testing-library": ">=7.0.0",
    "eslint-plugin-vue": ">=10.0.0",
    "svelte-eslint-parser": ">=1.0.0",
    "typescript": ">=5.0.0 <6.0.0",
    "vue-eslint-parser": ">=10.0.0"
  },
  "peerDependenciesMeta": {
    "@next/eslint-plugin-next": {
      "optional": true
    },
    "@ngrx/eslint-plugin": {
      "optional": true
    },
    "@tanstack/eslint-plugin-query": {
      "optional": true
    },
    "@tanstack/eslint-plugin-router": {
      "optional": true
    },
    "@vitest/eslint-plugin": {
      "optional": true
    },
    "angular-eslint": {
      "optional": true
    },
    "astro-eslint-parser": {
      "optional": true
    },
    "eslint": {
      "optional": true
    },
    "eslint-plugin-astro": {
      "optional": true
    },
    "eslint-plugin-jest": {
      "optional": true
    },
    "eslint-plugin-jsx-a11y": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-react-refresh": {
      "optional": true
    },
    "eslint-plugin-solid": {
      "optional": true
    },
    "eslint-plugin-svelte": {
      "optional": true
    },
    "eslint-plugin-testing-library": {
      "optional": true
    },
    "eslint-plugin-vue": {
      "optional": true
    },
    "svelte-eslint-parser": {
      "optional": true
    },
    "typescript": {
      "optional": true
    },
    "vue-eslint-parser": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
