{
  "name": "@javalce/eslint-config",
  "version": "0.7.0",
  "description": "Javier's eslint configuration",
  "keywords": [
    "eslint-config"
  ],
  "homepage": "https://github.com/javalce/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/javalce/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/javalce/eslint-config.git"
  },
  "license": "MIT",
  "author": "Javier Valero <javalce29@gmail.com>",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm typegen && tsup --format esm,cjs --clean --dts",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -t v -p angular",
    "dev": "npx @eslint/config-inspector --config eslint.config.ts",
    "lint": "eslint --flag unstable_ts_config .",
    "lint:fix": "eslint --flag unstable_ts_config --fix .",
    "prepack": "pnpm build",
    "prepare": "husky",
    "release": "bumpp && pnpm publish",
    "stub": "tsup --format esm",
    "typecheck": "tsc --noEmit",
    "typegen": "esno scripts/typegen.ts",
    "version": "pnpm changelog && git add CHANGELOG.md"
  },
  "lint-staged": {
    "*": "prettier -w --ignore-unknown"
  },
  "prettier": "@javalce/prettier-config",
  "dependencies": {
    "@babel/core": "^7.25.8",
    "@babel/eslint-parser": "^7.25.8",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.0",
    "@eslint/compat": "^1.2.0",
    "@eslint/js": "^9.12.0",
    "@stylistic/eslint-plugin": "^2.9.0",
    "eslint-config-flat-gitignore": "^0.3.0",
    "eslint-flat-config-utils": "^0.4.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import-x": "^4.3.1",
    "eslint-plugin-unicorn": "^56.0.0",
    "globals": "^15.11.0",
    "local-pkg": "^0.5.0",
    "typescript-eslint": "^8.9.0",
    "vue-eslint-parser": "^9.4.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/config-inspector": "^0.5.4",
    "@javalce/prettier-config": "^1.0.2",
    "@types/eslint": "^9.6.1",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.7.5",
    "bumpp": "^9.7.1",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.12.0",
    "eslint-typegen": "^0.3.2",
    "esno": "^4.8.0",
    "husky": "^9.1.6",
    "jiti": "^2.3.3",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "svelte": "^4.2.19",
    "tsup": "^8.3.0",
    "tsx": "^4.19.1",
    "typescript": "5.6.3"
  },
  "peerDependencies": {
    "@next/eslint-plugin-next": ">=12.3.0 <15.0.0-0",
    "@vitest/eslint-plugin": "^1.1.7",
    "eslint": "^9.12.0",
    "eslint-plugin-astro": "^1.3.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-solid": "^0.14.3",
    "eslint-plugin-svelte": "^2.44.1",
    "eslint-plugin-testing-library": "^6.3.0",
    "eslint-plugin-vue": "^9.29.0",
    "svelte-eslint-parser": "^0.41.1",
    "typescript": ">=4.8.0 <6.0.0"
  },
  "peerDependenciesMeta": {
    "@next/eslint-plugin-next": {
      "optional": true
    },
    "@vitest/eslint-plugin": {
      "optional": true
    },
    "eslint": {
      "optional": true
    },
    "eslint-plugin-astro": {
      "optional": true
    },
    "eslint-plugin-jest": {
      "optional": true
    },
    "eslint-plugin-jsx-a11y": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-solid": {
      "optional": true
    },
    "eslint-plugin-svelte": {
      "optional": true
    },
    "eslint-plugin-testing-library": {
      "optional": true
    },
    "eslint-plugin-vue": {
      "optional": true
    },
    "svelte-eslint-parser": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
